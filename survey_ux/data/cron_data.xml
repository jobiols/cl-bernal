<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ir_cron_compute_participacion_data" model="ir.cron">
        <field name="name">Calcular participacion en las encuestas de tipo simple_choice</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">15</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_survey_user_input_line"/>
        <field name="state">code</field>
        <field name="code">model.compute_participacion()</field>
    </record>
</odoo>
